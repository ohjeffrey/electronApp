{
  "private": true,
  "name": "electron_angular",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "gulp build && electron ./app",
    "test": "mocha",
    "test:ui": "gulp test",
    "compile": "rimraf dist",
    "pack": "npm run compile && build --dir",
    "dist": "npm run compile && build -wml"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "build": {
    "appId": "com.test.my.app",
    "category": "entertainment",
    "win": {
      "icon": "build/windows/icon.ico",
      "target": [
        "zip"
      ]
    },
    "nsis": {
      "oneClick": true,
      "installerHeaderIcon": "build/windows/setup-icon.ico"
    },
    "mac": {
      "icon": "build/osx/icon.icns"
    },
    "dmg": {
      "icon": "build/osx/dmg-icon.icns",
      "background": "build/osx/dmg-background.png"
    },
    "linux": {
      "icon": "build/windows/icon.ico",
      "target": [
        "deb", "tar.gz"
      ]
    }
  },
  "devDependencies": {
    "electron": "^1.4.5",
    "electron-builder": "^7.23.2",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4",
    "spectron": "^3.4.0",
    "estraverse": "~4.1.0",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~3.0.2",
    "gulp-angular-templatecache": "~1.8.0",
    "del": "~2.0.2",
    "lodash": "~3.10.1",
    "gulp-cssnano": "~2.1.1",
    "gulp-filter": "~3.0.1",
    "gulp-flatten": "~0.2.0",
    "gulp-eslint": "~1.0.0",
    "eslint-plugin-angular": "~0.12.0",
    "gulp-load-plugins": "~0.10.0",
    "gulp-size": "~2.0.0",
    "gulp-uglify": "~1.4.1",
    "gulp-useref": "~3.0.3",
    "gulp-util": "~3.0.6",
    "gulp-replace": "~0.5.4",
    "gulp-rename": "~1.2.2",
    "gulp-rev": "~6.0.1",
    "gulp-rev-replace": "~0.4.2",
    "gulp-htmlmin": "~1.3.0",
    "gulp-inject": "~3.0.0",
    "gulp-protractor": "~2.1.0",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-sass": "~2.0.4",
    "webpack-stream": "~2.1.1",
    "ng-annotate-loader": "0.0.10",
    "eslint-loader": "~1.1.0",
    "babel-core": "~6.7.4",
    "babel-loader": "~6.2.4",
    "babel-preset-es2015": "~6.6.0",
    "main-bower-files": "~2.9.0",
    "wiredep": "~2.2.2",
    "karma": "~0.13.10",
    "karma-jasmine": "~0.3.6",
    "karma-phantomjs-launcher": "~0.2.1",
    "phantomjs": "~1.9.18",
    "karma-phantomjs-shim": "~1.2.0",
    "karma-coverage": "~0.5.2",
    "karma-ng-html2js-preprocessor": "~0.2.0",
    "browser-sync": "~2.9.11",
    "browser-sync-spa": "~1.0.3",
    "http-proxy-middleware": "~0.9.0",
    "chalk": "~1.1.1",
    "uglify-save-license": "~0.4.1"
  },
  "packageIgnore": [
    ".tmp",
    "bower_components ",
    "e2e",
    "gulp",
    "node_modules",
    "src",
    "test",
    ".bowerrc",
    ".editorconfig",
    ".eslintrc",
    ".gitIgnore",
    "appveyor.yml",
    ".bower.json",
    "gulpfile.js",
    "karma.conf.js",
    "LICENSE.md",
    "protractor.conf.js",
    "README.md",
    ".DS_Store"
  ]
}